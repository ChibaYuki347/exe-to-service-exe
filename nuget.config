<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- Repo 直下の local-packages をローカルフィードとして使用（nuget.config からの相対パス） -->
    <add key="LocalPackages" value="local-packages" />
    <!-- フォールバックとして NuGet.org も有効化 -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>
